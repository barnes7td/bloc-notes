<%= form_tag upload_notes_path, method: :post, multipart: true do %>
  <div class="form-group">
    <%= label_tag :name %>
    <%= text_field_tag :name %>
  </div>

  <div class="form-group">
    <%= file_field_tag :file %>
  </div>

  <div class="form-group">
    <%= submit_tag "Upload" %>
  </div>
<% end %>
